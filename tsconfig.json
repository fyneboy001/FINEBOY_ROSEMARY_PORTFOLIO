{
  "compilerOptions": {
    "jsx": "react-jsx", // Or "react" if using older React
    "module": "esnext",
    "target": "es6",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  }
}
